#if defined(GTK_DISABLE_SINGLE_INCLUDES) && !defined (__GTK_H_INSIDE__) && !defined (GTK_COMPILATION)
#error "Only <gtk/gtk.h> can be included directly."
#endif

#ifndef __STLWRT_GTK4_VERSION_H__
#define __STLWRT_GTK4_VERSION_H__

/* This details which version of GTK that STLWRT is supposed to emulate.
   This is to be used only by programs which use STLWRT; STLWRT itself should
   use the run-time version information below instead.
 */
#define GTK_MAJOR_VERSION				(@GTK4_MAJOR_VERSION@)
#define GTK_MINOR_VERSION				(@GTK4_MINOR_VERSION@)
#define GTK_MICRO_VERSION				(@GTK4_MICRO_VERSION@)
#define GTK_BINARY_AGE					(@GTK4_BINARY_AGE@)
#define GTK_INTERFACE_AGE				(@GTK4_INTERFACE_AGE@)

#endif /* __STLWRT_GTK4_VERSION_H__ */
