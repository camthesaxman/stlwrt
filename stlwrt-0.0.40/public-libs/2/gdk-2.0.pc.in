prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
target=@stlwrx_target@

Name: STLWRX
Description: STLWRT Drawing Kit, GDK 2 emulation mode
Version: @GDK2_VERSION@
Requires: @STLWRX_PACKAGES@
Libs: -L${libdir} -lgdk-${target}-2.0 @STLWRX_EXTRA_LIBS@
Cflags: -I${includedir}/stlwrt @STLWRX_EXTRA_CFLAGS@ -DSTLWRT_GTK_COMPAT=2
